{
    "rewrites": [
        {
            "source": "/api/og/:slug",
            "destination": "/api/og/[slug]"
        },
        {
            "source": "/api/:path*",
            "destination": "/api/:path*"
        },
        {
            "source": "/((?!api/).*)",
            "destination": "/index.html"
        }
    ]
}
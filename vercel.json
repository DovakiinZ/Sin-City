{
    "rewrites": [
        {
            "source": "/api/og/:slug",
            "destination": "/api/og/[slug]"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "functions": {
        "api/**/*.ts": {
            "runtime": "@vercel/node@3"
        }
    }
}